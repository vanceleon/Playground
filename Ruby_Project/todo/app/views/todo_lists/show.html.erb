<p id="notice"><%= notice %></p>
<h2 class="todo_list_title"><%= @todo_lists.title %></h2>
<p class="todo_list_sub_title"><%= @todo_items.description %></p>
<div id="todo_items_wrapper">
 <%= render @todo_lists.todo_items %>
 <div id="form">
  <%= render "todo_items/form" %>
 </div>
</div>
<div class="links">
 <%= link_to 'Edit', edit_todo_list_path(@todo_lists) %> |
 <%= link_to 'Delete', todo_list_path(@todo_lists), method: :delete, data: { confirm: "Are you sure?" } %> |
 <%= link_to 'Back', todo_lists_path %>
</div>